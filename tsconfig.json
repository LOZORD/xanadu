{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "inlineSourceMap": true,
    "inlineSources": true,
    "allowJs": true,
    "pretty": false,
    "strictNullChecks": true,
    "traceResolution": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "exclude": [
    "dist",
    "node_modules",
    "coverage",
    "assets",
    "docs"
  ]
}
